{
  "$schema": "https://zeabur.com/schema/service.json",
  "name": "ski-platform-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "framework": "vite",
  "env": {
    "VITE_USER_CORE_API": {
      "default": "https://user-core.zeabur.app"
    },
    "VITE_RESORT_API": {
      "default": "https://resort-api.zeabur.app"
    },
    "VITE_ENV": {
      "default": "production"
    }
  }
}
