{
  "$schema": "https://zeabur.com/schema/service.json",
  "name": "resort-api",
  "dockerfile": "Dockerfile",
  "buildCommand": "pip install -r requirements.txt",
  "startCommand": "uvicorn app.main:app --host 0.0.0.0 --port 8000",
  "ports": [8000],
  "env": {
    "USER_CORE_API_URL": {
      "default": "http://user-core-api:8001/events"
    }
  }
}
